{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://html-file-manager-api.vercel.app/api/:path*" },
    { "source": "/uploads/:path*", "destination": "https://html-file-manager-api.vercel.app/uploads/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ]
}
